<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_tax_form_inherit" model="ir.ui.view">
            <field name="name">l10n_sv_pos_reports_xzm.account_tax.form</field>
            <field name="model">account.tax</field>
            <field name="inherit_id" ref="account.view_tax_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='active']" position="after">
                    <field name="es_tax_consumidor"/>
                    <field name="es_tax_contribuyente"/>
                    <field name="es_tax_exento"/>
                    <field name="es_tax_nosujeto"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>